{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoDatabase": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "ServiceLogs",
    "CollectionName": "Logs"
  },
  "ConnectionStrings": {
    "RabbitMQ": "ampq://guest:guest@localhost:8080/"
  },
  "ConsumerOptions": {
    "PrefetchSize": 0,
    "PrefetchCount": 1,
    "ChannelTarget": "Notifications"
  },
  "AllowedHosts": "*"
}
